{"sections":[],"identifier":{"interfaceLanguage":"swift","url":"doc:\/\/clinic.felt.EmbeddedCare\/documentation\/FeltEmbeddedCare\/Documentation"},"primaryContentSections":[{"kind":"content","content":[{"type":"heading","text":"Initialization","level":2,"anchor":"Initialization"},{"type":"paragraph","inlineContent":[{"text":"Primary interaction with the SDK is mediated through the ","type":"text"},{"type":"reference","identifier":"doc:\/\/clinic.felt.EmbeddedCare\/documentation\/FeltEmbeddedCare\/EmbeddedCare\/shared","isActive":true},{"type":"text","text":" singleton."}]},{"inlineContent":[{"type":"text","text":"At app launch, initialize the SDK with your SDK key using "},{"type":"reference","identifier":"doc:\/\/clinic.felt.EmbeddedCare\/documentation\/FeltEmbeddedCare\/EmbeddedCare\/initialize(sdkKey:)","isActive":true},{"text":".","type":"text"}],"type":"paragraph"},{"level":3,"type":"heading","anchor":"UIKit","text":"UIKit"},{"syntax":"swift","type":"codeListing","code":["import FeltEmbeddedCare","","let feltSDKKey = \"...\"","","class AppDelegate: UIApplicationDelegate {","    func application(_ application: UIApplication, didFinishLaunchingWithOptions launchOptions: [UIApplication.LaunchOptionsKey: Any]?) -> Bool {","        Task {","            do {","                try await EmbeddedCare.shared.initalize(sdkKey: feltSDKKey)","                \/\/ Update any UI waiting on embedded care initialization","            } catch {","                \/\/ Error will be thrown for invalid key","            }","        }","    }","}"]},{"anchor":"SwiftUI","text":"SwiftUI","level":3,"type":"heading"},{"code":["import FeltEmbeddedCare","","let feltSDKKey = \"...\"","","@main","struct ClientApp: App {","","    @State var embeddedCareInitialized = false","","    init() {","        Task {","            do {","                try await EmbeddedCare.shared.initialize(sdkKey: sdkKey)","                embeddedCareInitialized = true","            } catch {","                \/