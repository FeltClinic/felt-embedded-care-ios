{"hierarchy":{"paths":[["doc:\/\/clinic.felt.EmbeddedCare\/documentation\/FeltEmbeddedCare"]]},"identifier":{"url":"doc:\/\/clinic.felt.EmbeddedCare\/documentation\/FeltEmbeddedCare\/Documentation","interfaceLanguage":"swift"},"primaryContentSections":[{"kind":"content","content":[{"type":"heading","level":2,"text":"Initialization","anchor":"Initialization"},{"type":"paragraph","inlineContent":[{"text":"Primary interaction with the SDK is mediated through the ","type":"text"},{"identifier":"doc:\/\/clinic.felt.EmbeddedCare\/documentation\/FeltEmbeddedCare\/EmbeddedCare\/shared","isActive":true,"type":"reference"},{"text":" singleton.","type":"text"}]},{"type":"paragraph","inlineContent":[{"type":"text","text":"At app launch, initialize the SDK with your SDK key using "},{"type":"reference","identifier":"doc:\/\/clinic.felt.EmbeddedCare\/documentation\/FeltEmbeddedCare\/EmbeddedCare\/initialize(sdkKey:)","isActive":true},{"type":"text","text":"."}]},{"type":"heading","level":3,"text":"UIKit","anchor":"UIKit"},{"type":"codeListing","syntax":"swift","code":["import FeltEmbeddedCare","","let feltSDKKey = \"...\"","","class AppDelegate: UIApplicationDelegate {","    func application(_ application: UIApplication, didFinishLaunchingWithOptions launchOptions: [UIApplication.LaunchOptionsKey: Any]?) -> Bool {","        Task {","            do {","                try await EmbeddedCare.shared.initalize(sdkKey: feltSDKKey)","                \/\/ Update any UI waiting on embedded care initialization","            } catch {","                \/\/ Error will be thrown for invalid key","            }","        }","    }","}"]},{"type":"heading","level":3,"text":"SwiftUI","anchor":"SwiftUI"},{"type":"codeListing","syntax":"swift","code":["import FeltEmbeddedCare","","let feltSDKKey = \"...\"","","@main","struct ClientApp: App {","","    @State var embeddedCareInitialized = false","","    init() {","        Task {","            do {","                try await EmbeddedCare.shared.in